<?xml version="1.0" encoding="UTF-8"?>
<pages:ViewPage xmlns:pages="clr-namespace:MojaPasieka.View" 
x:TypeArguments="pages:TutorialPageModel" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MojaPasieka.View.TutorialPage">
	<ContentPage.Content>
		<StackLayout Padding="20">
			<pages:CardView>
				<pages:CardView.Content>
					<StackLayout>
						<Label Text="Witaj w tutorialu" />
						<Button Command="{Binding showCreator}" Text="Uruchom kreator" BackgroundColor="{x:Static pages:AppColors.AccentColor}" />
					</StackLayout>
				</pages:CardView.Content>
			</pages:CardView>
		</StackLayout>
	</ContentPage.Content>
</pages:ViewPage>
